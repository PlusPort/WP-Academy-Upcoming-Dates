{{#each getTrainingSessionListResult.SessionInfo}}
	<div class="edition">
		<div class="training-name">
			{{TrainingName}}
		</div>
		<div class="Location-address">
			{{LocationAddress}}, {{LocationCity}}
		</div>
		<div class="session-dates">
			{{#each SessionDates}}
				<div class="session-date">
					{{#each SessionDate}}
						<div class="date-from">{{../../../widget.pp_academy_date_from_prefix}} {{formatDate DateFrom}}</div>
						<div class="date-until">{{../../../widget.pp_academy_date_until_prefix}} {{formatDate DateUntil}}</div>
					{{/each}}
				</div>
			{{/each}}
		</div>
	</div>
{{/each}}
